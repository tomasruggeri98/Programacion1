// 9.Ingresar el tiempo trabajado por un operario y considerando que el valor de la hora es de $10 pesos,
// calcular su sueldo e imprimirlo.

Console.WriteLine(" - El monto de paga por hora es de $10 - ");
Console.WriteLine("Ingresar cantidad horas trabajadas para calcular sueldo: ");
int horas = int.Parse(Console.ReadLine());
int valor = 10;

int sueldo = valor * horas;
Console.WriteLine("El sueldo total del operario es de: " + sueldo);
