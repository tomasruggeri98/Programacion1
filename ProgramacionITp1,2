// 2.Ingresar tres valores,imprimir la suma total. (solo sabe sumar de a dos por vez).

Console.WriteLine("Ingrese valor1: ");
int Valor1 = int.Parse(Console.ReadLine());

Console.WriteLine("Ingrese valor2: ");
int Valor2 = int.Parse(Console.ReadLine());

int Suma1 = Valor1 + Valor2;

Console.WriteLine("sumaremos " + Suma1 + " m√°s el siguiente valor que ingrese");

Console.WriteLine("Ingrese valor3: ");
int Valor3 = int.Parse(Console.ReadLine());

int Sumas = Suma1 + Valor3;
Console.WriteLine(Sumas);
